{
  "name": "blockhead-consulting-website",
  "version": "1.0.0",
  "description": "Blockhead Consulting website with SPA functionality",
  "scripts": {
    "test": "npm run test:go && npm run test:js",
    "test:go": "go test -v ./...",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "test:js:coverage": "jest --coverage",
    "test:navigation": "jest navigation.test.js",
    "test:mobile": "jest mobile.test.js",
    "test:animations": "jest animations.test.js",
    "test:integration": "jest integration.test.js",
    "test:e2e": "jest e2e.test.js",
    "dev": "go run main.go",
    "build": "go build -o server main.go"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@testing-library/dom": "^10.4.0",
    "babel-jest": "^30.0.0-beta.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsdom": "^25.0.1",
    "puppeteer": "^21.0.0"
  }
}
